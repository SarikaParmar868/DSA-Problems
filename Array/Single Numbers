class Solution {
    public int singleNumber(int[] nums) {
        int ans = 0;
        for (int num : nums) {
            ans ^= num;
        }
        return ans;
    }

    public static void main(String[] args){
        int nums[] = {2,2,1};
        Solution s = new Solution();
        System.out.println(s.singleNumber(nums));
    }
}
